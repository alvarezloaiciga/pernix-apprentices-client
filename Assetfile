output "compiled"

input "source" do
  #match "js/**/*.coffee" do
    # Compile all .coffee files into.js
    #filter CoffeeScript
  #end

  # Select the JS generated by previous filters. 
  match "javascript/**/*.js" do
    concat "application.js"
  end

  match "css/**/*.css" do
    concat "application.css"
  end

  match "index.html" do
    copy
  end
end